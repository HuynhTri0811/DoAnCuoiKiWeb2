<div>
    <% for(let i = 0 ;i < cinema.length ; i++) { %>
        <p> <span> <%= cinema[i].dataValues.cinema_Name %> </span>
        <span> <%= cinema[i].dataValues.cinema_Type %> </span>
        <span> <%= cinema[i].dataValues.cinema_Length %></span>
        <span> <%= cinema[i].dataValues.cinema_Width %> </span>
        <span> <%= cinema[i].dataValues.Cineplex.cineplex_Name %> </span> 
        <span> <a href="<%= cinema[i].dataValues.cinema_ID %>">Cật nhập</a> </span>
        <span> <a href="/admin/delete/cinema/<%= cinema[i].dataValues.cinema_ID %>">Xóa</a></span>
    </p>
    <% } %>
</div>